    "netlify-cli": "^20.1.1",
    "prettier": "^2.1.1",
    "typescript": "^4.0.2",
    "webpack": "^5.94.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^5.2.1"
  }
}
